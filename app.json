{
  "name": "systemet-price-tracker",
  "description": "Email notifications for systembolaget",
  "repository": "https://github.com/mlonn/systemet-price-tracker",
  "success_url": "/",
  "addons": [
    {
      "plan": "mongolab",
      "as": "MONGODB"
    }
  ],
  "env": {
    "SEND_EMAIL": {
      "description": "Email address to send from",
      "required": true
    },
    "SEND_PASSWORD": {
      "description": "Email password",
      "required": true
    },
    "APP_URL": {
      "description": "<app-name>.herokuapp.com",
      "required": true
    }
  }
}
